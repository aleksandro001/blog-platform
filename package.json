    "lint": "eslint --debug ./src",
    "lint:fix": "eslint --debug ./src --fix --color",
    "format": "prettier --write src/**/*.js",
    "lint-staged": "lint-staged",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
