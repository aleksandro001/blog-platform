    "@hookform/resolvers": "^3.3.1",
    "@mui/icons-material": "^5.14.11",
    "@mui/material": "^5.14.11",
    "@reduxjs/toolkit": "^1.9.6",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "classnames": "^2.3.2",
